{
  "name": "@seacrest/rjs",
  "version": "0.0.8",
  "type": "module",
  "author": "Valery Seacrest",
  "license": {
    "type" : "MIT",
    "url" : "./License.md"
  },
  "types": "./dist/lib.d.ts",
  "main": "./dist/lib.umd.cjs",
  "module": "./dist/lib.js",
  "files": [ "./dist/", "./changelog.md" ],
  "exports": {
    ".": {
      "import": "./dist/lib.js",
      "require": "./dist/lib.umd.cjs"
    },
    "./common": {
      "import": "./dist/common.js",
      "require": "./dist/common.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "npm run build && node ./dist/lib.umd.cjs"
  },
  "devDependencies": {
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.6.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seacrest-sx/rjs"
  },
  "bugs": {
    "url": "https://github.com/seacrest-sx/rjs/issues"
  },
  "keywords": [
    "seacrest",
    "rust",
    "rust js",
    "rjs",
    "error",
    "ok",
    "compare",
    "result",
    "rust",
    "match",
    "variants",
    "enum",
    "option",
    "purge",
    "branch"
  ]
}
